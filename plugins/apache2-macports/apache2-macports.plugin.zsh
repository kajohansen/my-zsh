# commands to control local apache2 server installation
# paths are for osx installation via macports

alias apache2start='/opt/local/etc/LaunchDaemons/org.macports.apache2/apache2.wrapper start'
alias apache2stop='/opt/local/etc/LaunchDaemons/org.macports.apache2/apache2.wrapper stop'
alias apache2restart='/opt/local/etc/LaunchDaemons/org.macports.apache2/apache2.wrapper restart'
